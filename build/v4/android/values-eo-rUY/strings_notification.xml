    <string name="notification_keepalive_text">Konservu viva</string>
    <string name="notification_keepalive_description">Activate this switch if you experience problems with the app staying active in the background. Blokada will enable a permanent notification to prevent the system from killing it; you can hide it by choosing Notifications above and there Settings.</string>
    <!-- The blocked counter, displayed in the Keep Alive notification. -->
    <string name="notification_keepalive_title_new">Keep Alive (tap to hide)</string>
    <string name="notification_keepalive_text_new">Android requires to display a notification to keep the app running, but you are free to disable it in system settings.</string>
    <!-- The blocked counter, displayed in the Keep Alive notification. Parameter: blocked counter. -->
    <string name="notification_keepalive_title">%d blokita</string>
    <!-- The most recently blocked host, displayed in the Keep Alive notification. Parameter: blocked host name -->
    <string name="notification_keepalive_content">Lasta: %s</string>
    <!-- Content of the Keep Alive notification, if nothing was blocked yet, as in "Last: nothing yet". -->
    <string name="notification_keepalive_none">viglado</string>
