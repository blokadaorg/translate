    <string name="notification_keepalive_activating">Blokada startas. Tio povas preni momenton.</string>
    <string name="notification_keepalive_deactivate">Malaktivigi</string>
    <string name="notification_keepalive_deactivating">Blokada estas malaktivigita!</string>
