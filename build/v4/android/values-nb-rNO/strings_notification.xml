<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="notification_on_text">Varslinger</string>
    <string name="notification_on_description">Når du er på, vil Blokada vise en melding hver gang noe blir blokkert. Merk at dette er det samme som VPN-varslene som viser, og som ikke kan deaktiveres.&lt;br/&gt;Du kan konfigurere hvilke varsler som skal vises ved hjelp av innstillingene nedenfor.</string>
    <!-- Title of the notification that shows up every time something is blocked. -->
    <string name="notification_blocked_title">Blokkert</string>
    <!-- Content of the notification that shows up every time something is blocked. Parameter: blocked host name. -->
    <string name="notification_blocked_text">%s</string>
    <!-- Button to turn off notifications. -->
    <string name="notification_blocked_off">Ikke vis</string>
    <!-- Button to whitelist a host that was just blocked, visible in a notification. -->
    <string name="notification_blocked_whitelist">Hviteliste</string>
    <!-- Toast message that shows up once user adds a host to the whitelist, using a button from the notification. -->
    <string name="notification_blocked_whitelist_applied">Ferdig! Endringer kan ta noen sekunder.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_text">Hold i live</string>
    <string name="notification_keepalive_description">Aktiver denne bryteren hvis du opplever problemer med appen som holder seg aktiv i bakgrunnen. Blokada vil aktivere en permanent varsling for å hindre systemet i å drepe det. Du kan skjule det ved å velge varsler over og innstillinger</string>
    <!-- The blocked counter, displayed in the Keep Alive notification. -->
    <string name="notification_keepalive_title_new">Hold i live (trykk for å gjemme)</string>
    <string name="notification_keepalive_text_new">Android må vise en varsling for å holde appen kjører, men du har muligheten til å deaktivere den i systeminnstillinger.</string>
    <!-- The blocked counter, displayed in the Keep Alive notification. Parameter: blocked counter. -->
    <string name="notification_keepalive_title">%d blokkert</string>
    <!-- The most recently blocked host, displayed in the Keep Alive notification. Parameter: blocked host name -->
    <string name="notification_keepalive_content">Siste: %s</string>
    <!-- Content of the Keep Alive notification, if nothing was blocked yet, as in "Last: nothing yet". -->
    <string name="notification_keepalive_none">overvåker</string>
    <string name="notification_keepalive_activate">Aktiver</string>
    <string name="notification_keepalive_activating">Blokada starter. Dette kan ta et øyeblikk.</string>
    <string name="notification_keepalive_deactivate">Deaktiver</string>
    <string name="notification_keepalive_deactivating">Blokada ble deaktivert!</string>
    <string name="notification_expired_title">Blokada Plus er deaktivert</string>
    <string name="notification_expired_description">Kontoen din er ikke aktiv, og Blokada Plus er slått av. Standard Blokada funksjoner fungerer fint uten Blokada Plus VPN. Vurder imidlertid å aktivere kontoen for bedre beskyttelse og for å støtte prosjektet.</string>
    <string name="notification_lease_expired_title">Blokada Plus er deaktivert</string>
    <string name="notification_lease_expired_description">Det oppstod et problem med tilkoblingen din til Blokada Plus. Prøv å velge et annet gateway. Hvis problemet vedvarer, vennligst kontakt oss.</string>
    <string name="notification_channel_filtered">Annonseblokkering</string>
    <string name="notification_channel_announcements">Kunngjøringer</string>
    <string name="notification_channel_vpn">Blokada Plus</string>
    <string name="notification_channel_other">Annet</string>
</resources>
