#. [account.subscription_helptext] - Info about renewing cancelled subscriptions on info for active account
#. defaultMessage is:
#. A new subscription may be activated after the current cycle has expired.
#: build/messages/src/App.json
msgid "A new subscription may be activated after the current cycle has expired."
msgstr "Un nuovo abbonamento può essere attivato dopo la scadenza di quello attuale."

#. [account.subscription_status_active] - Subscription status parameter next to the account.subscription string
#. defaultMessage is:
#. ACTIVE
#: build/messages/src/App.json
msgid "ACTIVE"
msgstr "ATTIVO"

#. [account.title] - Title on info for active account
#. defaultMessage is:
#. Account ID
#: build/messages/src/App.json
msgid "Account ID"
msgstr "ID account"

#. [canceled.title] - Canceled page title
#. defaultMessage is:
#. Activation cancelled
#: build/messages/src/App.json
msgid "Activation cancelled"
msgstr "Attivazione annullata"

#. [account.activate_error] - Shown on credit card payment option upon errors loading payment info
#. defaultMessage is:
#. An error occured, please try later.
#: build/messages/src/Checkout.json
msgid "An error occured, please try later."
msgstr "Si è verificato un errore, riprova più tardi."

#. [account.subscription_switch] - Label next to checkbox switch for selecting to activate monthly subscription or not
#. defaultMessage is:
#. Auto renew monthly
#: build/messages/src/Checkout.json
msgid "Auto renew monthly"
msgstr "Rinnovo mensile automatico"

#. [account.subscription_price] - Price info above payment options
#. defaultMessage is:
#. Blokada VPN, €{price} per month
#: build/messages/src/Checkout.json
msgid "Blokada VPN, €{price} per month"
msgstr "Blokada VPN, {price}€ al mese"

#. [account.cancel_button] - Button for canceling a subscription on info for active account
#. defaultMessage is:
#. Cancel Subscription
#: build/messages/src/App.json
msgid "Cancel Subscription"
msgstr "Annulla Abbonamento"

#. [account.canceling] - Shown on Button for canceling a subscription while request is in progress
#. defaultMessage is:
#. Canceling {spinner}
#: build/messages/src/App.json
msgid "Canceling {spinner}"
msgstr "Annullo {spinner}"

#. [account.subscription_status_deactivating] - Subscription status parameter next to the account.subscription string
#. defaultMessage is:
#. DEACTIVATING
#: build/messages/src/App.json
msgid "DEACTIVATING"
msgstr "DISATTIVAZIONE"

#. [account.loading] - Shown before account info content as loaded
#. defaultMessage is:
#. Fetching account information {spinner}
#: build/messages/src/App.json
msgid "Fetching account information {spinner}"
msgstr "Recupero informazioni account {spinner}"

#. [account.subscription_status_inactive] - Subscription status parameter next to the account.subscription string
#. defaultMessage is:
#. INACTIVE
#: build/messages/src/App.json
msgid "INACTIVE"
msgstr "INATTIVO"

#. [account.subscription_status_loading] - Subscription status parameter next to the account.subscription string
#. defaultMessage is:
#. LOADING
#: build/messages/src/App.json
msgid "LOADING"
msgstr "CARICAMENTO"

#. [account.activate_loading] - Shown on account activate button while loading payment checkout page
#. defaultMessage is:
#. Loading {spinner}
#: build/messages/src/Checkout.json
msgid "Loading {spinner}"
msgstr "Caricamento {spinner}"

#. [notfound.title] - Not Found page title
#. defaultMessage is:
#. Not found
#: build/messages/src/App.json
msgid "Not found"
msgstr "Non trovato"

#. [account.subscription_card_header] - Title above credit card input
#. defaultMessage is:
#. Pay with card
#: build/messages/src/Checkout.json
msgid "Pay with card"
msgstr "Paga con carta"

#. [account.activate_once] - Button for paying one time when activating an account
#. defaultMessage is:
#. Pay €{price}
#: build/messages/src/Checkout.json
msgid "Pay €{price}"
msgstr "Paga {price}€"

#. [account.activate_subscription] - Button for subscribing to monthly payment for activating an account
#. defaultMessage is:
#. Pay €{price} each month
#: build/messages/src/Checkout.json
msgid "Pay €{price} each month"
msgstr "Paga {price}€ ogni mese"

#. [account.activate_processing] - Shown on account activate button while in the process of doing a payment charge
#. defaultMessage is:
#. Processing {spinner}
#: build/messages/src/Checkout.json
msgid "Processing {spinner}"
msgstr "Elaborazione {spinner}"

#. [account.error] - Generic error message when failing processing account info
#. defaultMessage is:
#. Sorry, an error occured. Please try again later.
#: build/messages/src/App.json
msgid "Sorry, an error occured. Please try again later."
msgstr "Ci spiace, si è verificato un errore. Per favore, riprova più tardi."

#. [account.subscription] - Subscription status on info for active account
#. defaultMessage is:
#. Subscription: {status}
#: build/messages/src/App.json
msgid "Subscription: {status}"
msgstr "Abbonamento: {status}"

#. [success.title] - Success page title
#. defaultMessage is:
#. Thanks!
#: build/messages/src/App.json
msgid "Thanks!"
msgstr "Grazie!"

#. [notfound.message] - Shown when accessing missing URL
#. defaultMessage is:
#. This page could not be displayed. If you experience any problems, please contact us at hello@blokada.org.
#: build/messages/src/App.json
msgid "This page could not be displayed. If you experience any problems, please contact us at hello@blokada.org."
msgstr "Questa pagina non può essere visualizzata. Per qualunque problema, per favore contattaci su hello@blokada.org."

#. [account.activate_helptext] - Help text shown when about to activate an account
#. defaultMessage is:
#. Write down the account ID before activating, it can be used to restore your subscription in case of data loss.
#: build/messages/src/App.json
msgid "Write down the account ID before activating, it can be used to restore your subscription in case of data loss."
msgstr "Prima di attivarlo, segnati da qualche parte l'ID account, può essere usato per ripristinare l'abbonamento in caso di perdita di dati."

#. [success.message] - Message after subscription success
#. defaultMessage is:
#. Your payment is being processed and your account will be activated shortly. If you experience any problems, please contact us at hello@blokada.org.
#: build/messages/src/App.json
msgid "Your payment is being processed and your account will be activated shortly. If you experience any problems, please contact us at hello@blokada.org."
msgstr "Il tuo pagamento è in fase di elaborazione e il tuo account sarà attivato a breve. Se riscontri problemi, per favore contattaci su hello@blokada.org."

#. [canceled.message] - Message after canceling the intent to create a new subscription
#. defaultMessage is:
#. Your payment was cancelled and you will not be charged. Your account is not active. If you experience any problems, please contact us at hello@blokada.org.
#: build/messages/src/App.json
msgid "Your payment was cancelled and you will not be charged. Your account is not active. If you experience any problems, please contact us at hello@blokada.org."
msgstr "Il tuo pagamento è stato annullato e non ti verrà addebitato. Il tuo account non è attivo. Se riscontri problemi, per favore contattaci su hello@blokada.org."

